//pseudocode
 
BEGIN
NUMBER n1,n2,n3
 
INPUT n1
INPUT n2
INPUT n3
 
IF n1>n2 AND n1>n3  THEN
     OUTPUT n1+ "is higher"
ELSE IF n2 > n3 THEN
     OUTPUT n2 + "is higher"
ELSE
     OUTPUT n3 "is higher"
ENDIF
 
END

//code

#include <iostream>
using namespace std;

int main() {
    
    int n1, n2, n3;

    cout << "Enter three numbers: ";
    cin >> n1 >> n2 >> n3;
    if(n1 >= n2 && n1 >= n3)
        cout << "Largest number: " << n1;

    else if(n2 >= n1 && n2 >= n3)
        cout << "Largest number: " << n2;
    
    else 
        cout << "Largest number: " << n3;
  
    return 0;
}
